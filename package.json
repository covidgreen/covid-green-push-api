{
  "name": "contact-tracing-push",
  "version": "1.0.0",
  "private": true,
  "description": "Push service for Contact Tracing",
  "main": "index.js",
  "scripts": {
    "create:env": "node -r fs -e \"fs.copyFileSync('.env.sample', '.env', fs.constants.COPYFILE_EXCL)\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "start": "node .",
    "start:dev": "nodemon --config .nodemonrc . | pino-pretty --colorize",
    "test": "jest --config .jest.json --coverage"
  },
  "dependencies": {
    "@nearform/sql": "^1.5.0",
    "aws-sdk": "^2.861.0",
    "crypto-random-string": "^3.3.1",
    "date-fns": "^2.19.0",
    "env-schema": "^3.0.1",
    "fastify": "^3.13.0",
    "fastify-autoload": "^3.6.0",
    "fastify-cors": "^5.2.0",
    "fastify-formbody": "^5.0.0",
    "fastify-jwt": "^2.3.0",
    "fastify-plugin": "^3.0.0",
    "fastify-postgres": "^3.5.0",
    "fastify-swagger": "^4.4.1",
    "fluent-schema": "^1.1.0",
    "http-errors": "^1.8.0",
    "nan": "^2.14.2",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "pg-range": "^1.1.0",
    "phone": "^2.4.21"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-prettier-standard": "^4.0.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^5.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.7.1",
    "prettier": "^2.2.1",
    "prettier-config-standard": "^4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": "prettier-config-standard"
}
